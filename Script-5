#!/bin/bash
echo "script untuk menerima input. penjumlahan dan perkalian"
echo "====================================================="
echo "1 :"
read x
echo "37 :"
read y
jmlh=`expr tx + ty`
kali=`expr tx \* ty`
echo "Hasil penjumlahan tx + ty = tjmlh"
echo "Hasil perkalian tx * ty = tkali"

